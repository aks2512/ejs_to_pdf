            <table>
              <thead>
                <th>25 - Tabela</th>
                <th> 26 - Código do Procedimento</th>
                <th>27 - Descrição</th>
                <th>28 - Qtde. Solict</th>
                <th>29 - Qtde. Aut</th>
              </thead>
              <tbody>
                <% for (var i = 0; i < procedures.length; i++) { %>
