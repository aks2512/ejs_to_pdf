    <!-- Dados do Atendimento -->
    <section>
      <h2 class="section__title">Dados do Atendimento</h2>
        <div class="row">
          <div class="box">
            <p class="box__title">46 - Tipo de Atendimento</p>
            <div class="box__content">
              <div class="itemsRow">
